{
	"schemaVersion": "0.1.0",
	"name": "GoogleBooks",
	"behavior": "create",
	"noteContentFormat": "{{content}}",
	"properties": [
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{selector:.bookinfo_sectionwrap .secondary}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings",
			"type": "multitext"
		},
		{
			"name": "image",
			"value": "{{image}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://books.google.co.jp/books"
	],
	"noteNameFormat": "{{title}}",
	"path": "Clippings"
}
